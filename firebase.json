{
	"functions": [
		{
			"source": ".",
			"codebase": "default",
			"disallowLegacyRuntimeConfig": true,
			"ignore": ["node_modules", ".git", "firebase-debug.log", "firebase-debug.*.log", "*.local"],
			"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"]
		}
	],
	"hosting": {
		"target": "kreyolpale",
		"public": "build/client",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"function": {
					"region": "us-central1",
					"functionId": "anegmaHandler"
				}
			}
		]
	},
	"emulators": {
		"auth": {
			"port": 9099
		},
		"functions": {
			"port": 5001
		},
		"firestore": {
			"port": 8080
		},
		"database": {
			"port": 9000
		},
		"hosting": {
			"port": 6001
		},
		"pubsub": {
			"port": 8085
		},
		"storage": {
			"port": 9199
		},
		"eventarc": {
			"port": 9299
		},
		"ui": {
			"enabled": true
		},
		"singleProjectMode": true
	},
	"firestore": {
		"database": "(default)",
		"location": "nam5",
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"storage": {
		"rules": "storage.rules"
	}
}
